<div id='firebaseui-auth-container' class='modal-auth'>
  <iframe name='q' style='display:none' src='about:blank'></iframe>

  <form action="javascript:void(0)" autocomplete="on" method="post">
    <input
      type='email'
      id='email_auth'
      class='modal-auth__input js-auth-register'
      placeholder='E-mail'
    />
    <input
      type='password'
      id='password-auth'
      class='modal-auth__input js-auth-login'
      placeholder='Password'
      title='minimum password length 6 characters'
    />
    <div class='button-container js-auth-button'>
      <button type='submit' class='button-auth js-btn-register' disabled />
      Registr
</button>
      {{! </input> }}
      <button type='submit' class='button-auth js-btn-login' disabled />
       Log in 
      </button>
    </div>
  </form>
</div>